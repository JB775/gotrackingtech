<html lang="en" class="demo-1 no-js">
	<head>
		<link rel="shortcut icon" href="../favicon.ico">
		<link rel="stylesheet" type="text/css" href="css/demo.css" />
        <link rel="stylesheet" type="text/css" href="css/component.css" />
	</head>
	<body>
	    <nav class="navbar navbar-static-top navbar-light bg-faded nav1">
	      <a href="/" class="navbar-brand"><%= image_tag 'logo1.png', class:'pull-left' %></a>
	      <ul class="nav navbar-nav">
	        <li class="nav-item">
	          <a class="nav-link" href="/">Home</a>
	        </li>
	        <li class="nav-item">
	          <a class="nav-link" href="/elogs">Features</a>
	        </li>
	        <li class="nav-item active">
	          <a class="nav-link" href="#">Contact <span class="sr-only">(current)</span></a>
	        </li>
	        <li class="nav-item">
	          <a class="nav-link" href="/about">About</a>
	        </li>
	      </ul>
	      <ul class="nav navbar-nav lognav">
		    <% if session[:user_id] %>
		      <li class="nav-item">
			    <a class="nav-link" href="/myaccount">My Account</a>
			  </li>
		      <li class="nav-item pref">
		        <a  href="/preferences">Preferences</a>
		      </li>
		      <li class="nav-item logoutnav">
		        <a  href="/logout"><a class="btn btn-primary" href="/logout" role="button">Logout</a></a>
		      </li>
		     <% else %>
		      <li class="nav-item">
		        <a  href="/newuser">Create Account</a>
		      </li>
		      <li class="nav-item">
		        <a  href="/login"><a class="btn btn-primary" href="/login" role="button">Login</a></a>
		      </li>
		    <% end %>
		  </ul>
		</nav>
	    <div class="contact_body">
			<div class="container">
				<!-- Top Navigation -->
				
				<!-- <div class="cctop clearfix">
				</div> -->

				<header class="ccheader">
					<h1>Request Information</h1>	
					
				</header>
				<div class="wrapper">
	            	
					<div class="cclogin simple">
	                	<h2>Contact Us</h2>
	                	
	                	<%= form_for @contact, :url => contacts_path, method: :post do |f| %>
						  <p class="half">
						  	<%= f.text_field :fname, :placeholder => 'First Name' %>
						  </p>
						  <p class="half last">
						  	<%= f.text_field :lname, :placeholder => 'Last Name' %>
						  </p>
						  <p>
						  	<%= f.text_field :email, :placeholder => 'Email' %>
						  </p>
						  <p>
						  	<%= f.text_field :phone, :placeholder => 'Phone Number' %>
						  </p>
						  <p>
						  	<%= f.text_field :company, :placeholder => 'Business Name' %>
						  </p>
						  <p class="half">
						  	<%= f.text_field :city, :placeholder => 'Business City' %>
						  </p>
						  <p class="half last">
						  	<%= select_tag(:state, options_for_select([
						  		['State', 'State'], 
						  		['AK', 'AK'],
							    ['AL', 'AL'],
							    ['AR', 'AR'],
							    ['AZ', 'AZ'],
							    ['CA', 'CA'],
							    ['CO', 'CO'],
							    ['CT', 'CT'],
							    ['DC', 'DC'],
							    ['DE', 'DE'],
							    ['FL', 'FL'],
							    ['GA', 'GA'],
							    ['HI', 'HI'],
							    ['IA', 'IA'],
							    ['ID', 'ID'],
							    ['IL', 'IL'],
							    ['IN', 'IN'],
							    ['KS', 'KS'],
							    ['KY', 'KY'],
							    ['LA', 'LA'],
							    ['MA', 'MA'],
							    ['MD', 'MD'],
							    ['ME', 'ME'],
							    ['MI', 'MI'],
							    ['MN', 'MN'],
							    ['MO', 'MO'],
							    ['MS', 'MS'],
							    ['MT', 'MT'],
							    ['NC', 'NC'],
							    ['ND', 'ND'],
							    ['NE', 'NE'],
							    ['NH', 'NH'],
							    ['NJ', 'NJ'],
							    ['NM', 'NM'],
							    ['NV', 'NV'],
							    ['NY', 'NY'],
							    ['OH', 'OH'],
							    ['OK', 'OK'],
							    ['OR', 'OR'],
							    ['PA', 'PA'],
							    ['RI', 'RI'],
							    ['SC', 'SC'],
							    ['SD', 'SD'],
							    ['TN', 'TN'],
							    ['TX', 'TX'],
							    ['UT', 'UT'],
							    ['VA', 'VA'],
							    ['VT', 'VT'],
							    ['WA', 'WA'],
							    ['WI', 'WI'],
							    ['WV', 'WV'],
							    ['WY', 'WY'] ])) %>
						  </p>
						  <p>
						  	<%= f.text_field :job_title, :placeholder => 'Job Title' %>
						  </p>
						  <p>
						  	<%= f.text_field :num_employees, :placeholder => 'Number of Employees' %>
						  </p>
						  <p>
						  	<%= f.text_field :message, :placeholder => 'Question/Message' %>
						  </p>
						  <p>
						  	<%= f.submit "Submit"%>
						  </p>
						<% end %>
	                    
	                    <!-- <p><input type="checkbox" name="remember">I agree to the Terms and Conditions.</p> -->
	                    <!-- <p><input type="checkbox" name="remember">I want to receive news and announcements from GoTrackingTech.</p> -->
	                </div>
	                
				</div>
				
			</div><!-- /container -->
		</div>
		
	</body>
</html>
